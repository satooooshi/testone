{
  "name": "groupware-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "typeorm migration:run && node main",
    "start:local": "dist/node main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:init": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateInit",
    "seed:user": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateUser",
    "seed:news": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateNews",
    "seed:group": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateChatGroup",
    "seed:eventIntroduction": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateEventIntroduction",
    "seed:event": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateEvents",
    "seed:tags": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateTags",
    "seed:question": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateQAQuestions",
    "migration:generate": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.14.0",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-socket.io": "^8.2.4",
    "@nestjs/typeorm": "^8.0.1",
    "@nestjs/websockets": "^8.2.4",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.0.1",
    "class-transformer": "<=0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.6",
    "faker": "^5.5.3",
    "handlebars": "^4.7.7",
    "json2csv": "^5.0.6",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "markdown-it": "^12.2.0",
    "mysql2": "^2.2.5",
    "node-pushnotifications": "^1.7.3",
    "nodemailer": "^6.6.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.4.0",
    "typeorm": "^0.2.43",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.22",
    "@types/json2csv": "^5.0.3",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.175",
    "@types/luxon": "^2.0.4",
    "@types/markdown-it": "^12.2.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.36",
    "@types/node-pushnotifications": "^1.0.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
